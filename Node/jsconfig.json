{
    "compilerOptions": {
        "checkJs": true,
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@configs/*": [
                "./src/configs/*"
            ],
            "@exceptions/*": [
                "./src/exceptions/*"
            ],
            "@enums/*": [
                "./src/enums/*"
            ],
            "@hooks/*": [
